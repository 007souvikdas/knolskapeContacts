{
  "env": {
      "commonjs": true,
      "es2020": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 11
  },
  "rules": {
      "indent": ["error", 4],
      "no-underscore-dangle": "off",
      "max-len": ["error", 150],
      "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"]
  }
}
